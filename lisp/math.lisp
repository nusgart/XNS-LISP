;; basic ops
(define * mult)
(define + plus)
(define - minus)
(define / divide)
;; mathematical constants -- self documenting
(define +pi+ 3.141592653589793)
(define +pi/2+ 1.57079632679489661923)
(define +pi/4+ 0.78539816339744830962)
(define +1/pi+ 0.31830988618379067154)
(define +2/pi+ 0.63661977236758134308)
(define +2/sqrt-pi+ 1.12837916709551257390)
(define +e+ 2.7182818284590452354)
(define +log2_e+ 1.4426950408889634074)
(define +log10_e+ 0.43429448190325182765)
(define +ln_2+ 0.69314718055994530942)
(define +ln_10+ 2.30258509299404568402)
(define +sqrt2+ 1.41421356237309504880)
(define +sqrt1/2+ 0.70710678118654752440)